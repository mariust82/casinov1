<?xml version="1.0"?>
<!DOCTYPE xml>
<xml>
  <application>
    <default_character_encoding>UTF-8</default_character_encoding>
    <default_page>index</default_page>
    <default_extension>html</default_extension>
    <auto_routing>0</auto_routing>
    <paths>
      <controllers>application/controllers</controllers>
      <wrappers>application/wrappers</wrappers>
      <views>application/views</views>
      <listeners>application/listeners</listeners>
      <dao>application/models/dao</dao>
      <tags>application/taglib</tags>
      <tms_variables>application/models/tms_variables</tms_variables>
      <compilations>
        <lucinda>/tmp/compilations_casinolists</lucinda>
        <dev>/tmp/compilations_dev</dev>
        <live>/tmp/compilations_live</live>
        <alex>/tmp/compilations_live</alex>
      </compilations>
    </paths>
    <environments>
      <lucinda>www.casinoslists.local</lucinda>
      <dev>dev.casinoslists.com</dev>
      <live>www.casinoslists.com</live>
      <alex>lists.local</alex>
    </environments>
    <templates_extension>html</templates_extension>
  </application>
  <formats>
    <format extension="html" content_type="text/html" class="BypassWrapper" charset="UTF-8"/>
    <format extension="json" content_type="application/json" class="JsonWrapper"/>
    <format extension="xml" content_type="text/xml" class="SitemapWrapper"/>
  </formats>
  <listeners>
    <listener class="EnvironmentDetector"/>
    <listener class="AsyncListener"/>
    <listener class="SQLDataSourceInjector"/>
    <listener class="QueryProfilingListener"/>
    <listener class="ErrorListener"/>
    <listener class="RebrandingListener"/>
    <listener class="IPDetectionListener"/>
    <listener class="MobileDetectionListener"/>
    <listener class="TMSListener"/>
    <listener class="ViewLanguageResolver"/>
  </listeners>
  <errors>
    <lucinda>
         <reporters>
                    <sql table="bugs"/>
                </reporters>
      <renderer display_errors="1">
          <html/>
          <json/>
      </renderer>
    </lucinda>
    <dev>
       <reporters>
            <async/>
        </reporters>
      <renderer display_errors="1">
        <html/>
        <json/>
      </renderer>
    </dev>
    <live>
      <reporters>
        <async/>
      </reporters>
      <renderer display_errors="0">
        <html/>
        <json/>
      </renderer>
    </live>
    <alex>
      <reporters>
        <async/>
      </reporters>
      <renderer display_errors="1">
        <html/>
        <json/>
      </renderer>
    </alex>
  </errors>
  <routes>
    <route url="index" controller="IndexController" view="index"/>
    <route url="banking/(name)" controller="CasinosByBankingMethodController" view="casinos" rebrand_tables="banking_methods"/>
    <route url="banking" controller="BankingMethodsController" view="banking-methods"/>
    <route url="casinos/(name)" controller="CasinosByLabelController" view="casinos"/>
    <route url="casinos" controller="CasinoLabelsController" view="casino-labels"/>
    <route url="reviews/(name)-review" controller="CasinoInfoController" view="casino-info" rebrand_tables="casinos"/>
    <route url="visit/(name)" controller="CasinoVisitController"/>
    <route url="warn/(name)" controller="CasinoWarningController" view="casino-warning"/>
    <route url="bonus-list/(name)" controller="CasinosByBonusTypeController" view="casinos" rebrand_tables="bonus_types"/>
    <route url="bonus-list" controller="BonusesController" view="bonus-types"/>
    <route url="countries-list/(name)" controller="CasinosByCountryController" view="casinos"/>
    <route url="countries" controller="CountriesController" view="countries"/>
    <route url="compatability/(name)" controller="CasinosByCompatibilityController" view="casinos"/>
    <route url="compatability" controller="CompatibilityController" view="compatibility"/>
    <route url="softwares/(name)" controller="CasinosBySoftwareController" view="casinos" rebrand_tables="game_manufacturers"/>
    <route url="softwares" controller="SoftwaresController" view="softwares"/>
    <route url="games/(type)" controller="GamesByTypeController" view="games"/>
    <route url="games" controller="GameTypesController" view="game-types"/>
    <route url="play/(name)" controller="GameInfoController" view="game-info" rebrand_tables="games"/>
    <route url="play-counter" controller="GamePlayCounterController" extension="json"/>
    <route url="features/(name)" controller="CasinosByFeatureController" view="casinos"/>
    <route url="features" controller="FeaturesController" view="features"/>
    <route url="privacy" controller="PrivacyController" view="privacy-policy"/>
    <route url="terms" controller="TermsController" view="terms-and-conditions"/>
    <route url="contact" controller="ContactController" view="contact-us"/>
    <route url="contact/send" controller="ContactSendController" extension="json"/>
    <route url="casinos-filter/(page)" controller="CasinosFilterController" view="casinos-list"/>
    <route url="casinos-filter" controller="CasinosFilterController" view="casinos-list"/>
    <route url="games-filter/(page)" controller="GamesFilterController" view="games-list"/>
    <route url="games-filter" controller="GamesFilterController" view="games-list"/>
    <route url="search" controller="SearchController" extension="json"/>
    <route url="search/advanced" controller="SearchAdvancedController" view="search"/>
    <route url="search/more-lists/(page)" controller="SearchMoreListsController" extension="json"/>
    <route url="search/more-casinos/(page)" controller="SearchMoreCasinosController" extension="json"/>
    <route url="search/more-games/(page)" controller="SearchMoreGamesController" extension="json"/>
    <route url="casino/rate" controller="CasinoRateController" extension="json"/>
    <route url="casino/review-write" controller="CasinoReviewWriteController" extension="json"/>
    <route url="casino/review-like" controller="CasinoReviewLikeController" extension="json"/>
    <route url="casino/more-reviews/(name)/(page)" controller="CasinoMoreReviewsController" view="reviews"/>
    <route url="newsletter/subscribe" controller="NewsletterSubscribeController" extension="json"/>
    <route url="casino/bonus" controller="CasinoBonusController" extension="json"/>
    <route url="404" controller="PageNotFoundController" view="404"/>

    <route url="sitemap_cas_bank.php" controller="sitemap/BankingMethodsController" extension="xml"/>
    <route url="sitemap_cas_bon.php" controller="sitemap/BonusTypesController" extension="xml"/>
    <route url="sitemap_cas_cas.php" controller="sitemap/CasinoLabelsController" extension="xml"/>
    <route url="sitemap_cas_con.php" controller="sitemap/CountriesController" extension="xml"/>
    <route url="sitemap_cas_feat.php" controller="sitemap/FeaturesController" extension="xml"/>
    <route url="sitemap_cas_games.php" controller="sitemap/GameTypesController" extension="xml"/>
    <route url="sitemap_cas_os.php" controller="sitemap/CompatibilityController" extension="xml"/>
    <route url="sitemap_cas_soft.php" controller="sitemap/SoftwaresController" extension="xml"/>
    <route url="sitemap_lists.php" controller="sitemap/PagesController" extension="xml"/>
    <route url="sitemap_main.php" controller="sitemap/IndexController" extension="xml"/>
    <route url="sitemap_spec_cas.php" controller="sitemap/CasinosController" extension="xml"/>
    <route url="sitemap_spec_games.php" controller="sitemap/GamesController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_bank_ps.php" controller="sitemap/BankingMethodsController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_bon_ps.php" controller="sitemap/BonusTypesController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_cas_ps.php" controller="sitemap/CasinoLabelsController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_con_ps.php" controller="sitemap/CountriesController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_feat_ps.php" controller="sitemap/FeaturesController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_games_ps.php" controller="sitemap/GameTypesController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_os_ps.php" controller="sitemap/CompatibilityController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_cas_soft_ps.php" controller="sitemap/SoftwaresController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_lists_ps.php" controller="sitemap/PagesController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_main_ps.php" controller="sitemap/IndexController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_spec_cas_ps.php" controller="sitemap/CasinosController" extension="xml"/>
    <route url="sitemaps_ps/sitemap_spec_games_ps.php" controller="sitemap/GamesController" extension="xml"/>
  </routes>
